# Supabase Configuration
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_ANON_KEY=tu-anon-key
SUPABASE_SERVICE_KEY=tu-service-key

# Server Configuration
PORT=3000
NODE_ENV=development

# n8n Webhook Security (opcional)
N8N_WEBHOOK_SECRET=tu-secreto-compartido

# Google Sheets Integration
# Deploy el Apps Script (ver google-apps-script/Code.gs) y pega la URL aquí
GOOGLE_SHEETS_SCRIPT_URL=

# AI Assistant (Anthropic Claude)
# Obtén tu API key en: https://console.anthropic.com/settings/keys
# OPCIONAL: Si no se configura, el asistente IA no estará disponible
ANTHROPIC_API_KEY=
